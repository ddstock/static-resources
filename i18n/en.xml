<?xml version="1.0" encoding="UTF-8"?>
<application>
  <Navigator>
    <SiteSelector>
      <Label>Sites</Label>
      <Item>
        <UnderMaintenance>Under maintenance</UnderMaintenance>
        <NotImplemented>Not implemented yet</NotImplemented>
      </Item>
    </SiteSelector>

    <Feedback>
      <Label>Feedback</Label>
    </Feedback>

    <About>
      <Label>About</Label>
    </About>

    <LanguagePreference>
      <Label>Language preference</Label>

      <Languages>
        <en>English</en>
        <es>Spanish</es>
        <ja>Japanese</ja>
        <vi>Vietnamese</vi>
      </Languages>
    </LanguagePreference>

    <Account>
      <Balance>Balance</Balance>
      <Functions>
        <Login>login</Login>
        <Logout>logout</Logout>
      </Functions>
    </Account>
  </Navigator>

  <Dialog>
    <Login>
      <Label>Login</Label>
      <Username>Username</Username>
      <Password>Password</Password>
      <Button>Login</Button>
    </Login>

    <SavedInfo>
      <Label>Detail</Label>
      <Detail>
        <Name>Name</Name>
        <Description>Description</Description>
        <Source>Source</Source>
        <PurchaseAt>Purchased at</PurchaseAt>
        <Expire>Expire</Expire>
        <Expired>Expired</Expired>
        <Download>Download</Download>
        <Remove>Remove</Remove>
        <Renew>Renew</Renew>
      </Detail>
    </SavedInfo>

    <Feedback>
      <Label>Feedback</Label>
      <Inputs>
        <Name>Name</Name>
        <Message>Message</Message>
      </Inputs>
    </Feedback>

    <About>
      <Label>About</Label>
      <Content>
        <Version>Version</Version>

        <Text>Not thing here</Text>
      </Content>
    </About>
  </Dialog>

  <Evaluator>
    <Evaluate>evaluate</Evaluate>
    <InputGroup>
      <Label>URL</Label>
      <PlaceHolder>Enter the URL of the image. If it appears as a pop-up, open it in a new tab.</PlaceHolder>
      <Selected>selected</Selected>
    </InputGroup>
  </Evaluator>

  <Preview>
    <Label>
      <Text>Preview</Text>
      <Total>Total</Total>
    </Label>
    <Button>
      <Default>Save</Default>
      <Insufficient>Insufficient balance</Insufficient>
      <Saving>Saving</Saving>
    </Button>
    <Empty>Nothing here</Empty>
    <Fee>Fee</Fee>
  </Preview>

  <Saved>
    <Label>Saved</Label>
    <Empty>Nothing here</Empty>
  </Saved>

  <Api>
    <Evaluate>
      <Errors>
        <None></None>
        <NotFound>Image not found (make sure the URL is correct)</NotFound>
        <Invalid>Invalid URL</Invalid>
        <Internal>Service is currently unavailable</Internal>
        <External>External service is currently unavailable</External>
        <Unhandled>Unknown error</Unhandled>
      </Errors>
    </Evaluate>
  </Api>

  <Locker>
    <Auth>You have to log in first to use this function</Auth>
    <Site>You have to select target site first to use this function</Site>
    <Swipe>(Swipe to right)</Swipe>
  </Locker>

  <DateTime>
    <Months>
      <Month>Jan</Month>
      <Month>Feb</Month>
      <Month>Mar</Month>
      <Month>Apr</Month>
      <Month>May</Month>
      <Month>Jun</Month>
      <Month>Jul</Month>
      <Month>Aug</Month>
      <Month>Sep</Month>
      <Month>Oct</Month>
      <Month>Nov</Month>
      <Month>Dec</Month>
    </Months>
  </DateTime>
</application>
