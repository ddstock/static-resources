<?xml version="1.0" encoding="UTF-8"?>
<application>
  <SiteSelector>
    <Header>Các trang</Header>
    <Item>
      <UnderMaintenance>Bảo trì</UnderMaintenance>
      <NotImplemented>Chưa hoạt động</NotImplemented>
    </Item>
  </SiteSelector>

  <LanguagePreference>
    <Label>Tùy chọn ngôn ngữ</Label>

    <Languages>
      <en>Tiếng Anh</en>
      <vi>Tiếng Việt</vi>
      <ja>Tiếng Nhật</ja>
    </Languages>
  </LanguagePreference>

  <Account>
    <Balance>Số dư</Balance>
    <Functions>
      <Login>Đăng nhập</Login>
      <Logout>Thoát</Logout>
    </Functions>
  </Account>

  <Dialog>
    <Login>
      <Label>Đăng nhập</Label>
      <Username>Tên người dùng</Username>
      <Password>Mật khẩu</Password>
      <Button>Đăng nhập</Button>
    </Login>

    <SavedInfo>
      <Label>Chi tiết</Label>
      <Detail>
        <Name>Tên</Name>
        <Description>Mô tả</Description>
        <Source>Nguồn</Source>
        <PurchaseAt>Mua ngày</PurchaseAt>
        <Expire>Hết hạn</Expire>
        <Expired>Đã hết hạn</Expired>
        <Download>Tải về</Download>
        <Remove>Xóa</Remove>
        <Renew>Tải lại</Renew>
      </Detail>
    </SavedInfo>
  </Dialog>

  <Evaluator>
    <Evaluate>đánh giá</Evaluate>
    <InputGroup>
      <Label>URL</Label>
      <PlaceHolder>Nhập đường dẫn hình ảnh, nếu hình ảnh ở dạng popup, bạn hãy mở nó bằng tab mới.</PlaceHolder>
      <Selected>đã chọn</Selected>
    </InputGroup>
  </Evaluator>

  <Preview>
    <Label>
      <Text>Xem trước</Text>
      <Total>Tổng</Total>
    </Label>
    <Button>
      <Default>Lưu</Default>
      <Insufficient>Số dư không đủ</Insufficient>
      <Saving>Đang lưu</Saving>
    </Button>
    <Empty>Không có gì</Empty>
    <Fee>Phí</Fee>
  </Preview>

  <Saved>
    <Label>Ảnh đã lưu</Label>
    <Empty>Không có gì</Empty>
  </Saved>

  <Api>
    <Evaluate>
      <Errors>
        <None></None>
        <NotFound>Không tìm thấy hình ảnh (hãy chắc chắn bạn gõ đúng URL)</NotFound>
        <Invalid>URL không hợp lệ</Invalid>
        <Internal>Dịch vụ hiện không khả dụng</Internal>
        <External>Dịch vụ ngoài hiện không khả dụng</External>
        <Unhandled>Lỗi chưa biết</Unhandled>
      </Errors>
    </Evaluate>
  </Api>

  <Locker>
    <Auth>Bạn phải đăng nhập để sử dụng tính năng này</Auth>
    <Site>Bạn phải chọn trang web sản phẩn trước để sử dụng tính năng này</Site>
    <Swipe>(Kéo sang phải)</Swipe>
  </Locker>

  <DateTime>
    <Months>
      <Month>Tháng 1</Month>
      <Month>Tháng 2</Month>
      <Month>Tháng 3</Month>
      <Month>Tháng 4</Month>
      <Month>Tháng 5</Month>
      <Month>Tháng 6</Month>
      <Month>Tháng 7</Month>
      <Month>Tháng 8</Month>
      <Month>Tháng 9</Month>
      <Month>Tháng 10</Month>
      <Month>Tháng 11</Month>
      <Month>Tháng 12</Month>
    </Months>
  </DateTime>
</application>
