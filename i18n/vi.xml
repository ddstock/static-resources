<?xml version="1.0" encoding="UTF-8"?>
<application>
  <Navigator>
    <SiteSelector>
      <Label>Các trang</Label>
      <Item>
        <UnderMaintenance>Bảo trì</UnderMaintenance>
        <NotImplemented>Chưa hoạt động</NotImplemented>
      </Item>
    </SiteSelector>

    <Feedback>
      <Label>Phản hồi</Label>
    </Feedback>

    <About>
      <Label>Giới thiệu</Label>
    </About>

    <LanguagePreference>
      <Label>Tùy chọn ngôn ngữ</Label>

      <Languages>
        <en>Tiếng Anh</en>
        <ja>Tiếng Nhật</ja>
        <vi>Tiếng Việt</vi>
      </Languages>
    </LanguagePreference>

    <Account>
      <Balance>Số dư</Balance>
      <Functions>
        <Login>đăng nhập</Login>
        <Logout>đăng xuất</Logout>
      </Functions>
    </Account>
  </Navigator>

  <Dialog>
    <Login>
      <Label>Đăng nhập</Label>
      <Username>Tên người dùng</Username>
      <Password>Mật khẩu</Password>
      <Button>Đăng nhập</Button>
    </Login>

    <SavedInfo>
      <Label>Chi tiết</Label>
      <Detail>
        <Name>Tên</Name>
        <Description>Mô tả</Description>
        <Source>Nguồn</Source>
        <PurchaseAt>Mua lúc</PurchaseAt>
        <Expire>Hết hạn</Expire>
        <Expired>Đã hết hạn</Expired>
        <Download>Tải xuống</Download>
        <Remove>Xóa</Remove>
        <Renew>Mua lại</Renew>
      </Detail>
    </SavedInfo>

    <Feedback>
      <Label>Phản hồi</Label>
      <Inputs>
        <Name>Tên</Name>
        <Message>Tin nhắn</Message>
      </Inputs>

      <Actions>
        <Close>Đóng</Close>
        <Send>Gửi</Send>
      </Actions>
    </Feedback>

    <About>
      <Label>Giới thiệu</Label>
      <Content>
        <Version>Phiên bản</Version>

        <Text>Không có gì ở đây</Text>
      </Content>
    </About>
  </Dialog>

  <Evaluator>
    <Evaluate>đánh giá</Evaluate>
    <InputGroup>
      <Label>URL</Label>
      <PlaceHolder>Nhập đường dẫn hình ảnh, nếu hình ảnh ở dạng pop-up, bạn hãy mở nó bằng tab mới.</PlaceHolder>
    </InputGroup>
  </Evaluator>

  <Preview>
    <Label>
      <Text>Xem trước</Text>
      <Total>Tổng</Total>
    </Label>
    <Button>
      <Default>Lưu hết</Default>
      <Insufficient>Số dư không đủ</Insufficient>
      <Saving>Đang lưu</Saving>
    </Button>
    <Empty>Không có gì ở đây</Empty>
    <Fee>Phí</Fee>
  </Preview>

  <Saved>
    <Label>Đã lưu</Label>
    <Empty>Không có gì ở đây</Empty>
  </Saved>

  <Api>
    <Evaluate>
      <Errors>
        <None></None>
        <NotFound>Không tìm thấy hình ảnh (hãy chắc chắn bạn gõ đúng URL)</NotFound>
        <Invalid>URL không hợp lệ</Invalid>
        <Internal>Dịch vụ hiện không khả dụng</Internal>
        <External>Dịch vụ ngoài hiện không khả dụng</External>
        <Unhandled>Lỗi chưa biết</Unhandled>
      </Errors>
    </Evaluate>
  </Api>

  <Locker>
    <Auth>Bạn phải đăng nhập để sử dụng tính năng này</Auth>
    <Site>Bạn phải chọn trang web sản phẩn trước để sử dụng tính năng này</Site>
    <Swipe>(Vuốt sang phải)</Swipe>
  </Locker>

  <DateTime>
    <Months>
      <Month>Tháng 1</Month>
      <Month>Tháng 2</Month>
      <Month>Tháng 3</Month>
      <Month>Tháng 4</Month>
      <Month>Tháng 5</Month>
      <Month>Tháng 6</Month>
      <Month>Tháng 7</Month>
      <Month>Tháng 8</Month>
      <Month>Tháng 9</Month>
      <Month>Tháng 10</Month>
      <Month>Tháng 11</Month>
      <Month>Tháng 12</Month>
    </Months>
  </DateTime>
</application>
